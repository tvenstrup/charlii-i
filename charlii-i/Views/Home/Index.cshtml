@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="photo-stream" class="carousel slide" data-ride="carousel" data-interval="10000">
    <ol class="carousel-indicators">
        <li data-target="#photo-stream" data-slide-to="0" class="active"></li>
        <li data-target="#photo-stream" data-slide-to="1"></li>
    </ol>

    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/Content/Images/concert.jpg" alt=""/>
        </div>
        <div class="item">
            <img src="~/Content/Images/concert-color.jpg" alt=""/>
        </div>
    </div>
</div>
<div id="news" class="jumbotron">
    <h2>Hør den nyeste låt</h2>
</div>

